<vcss>
  <!--
   Each <vcs> has a name atribute (name of vcs used). 
   -->
  
  <vcs 
    name="svn">

    <commands>
      <command> add </command>
      <command> blame </command>
      <command> cat </command>
      <command> commit </command>
      <command> diff </command>
      <command> help </command>
      <command> info </command>
      <command> log </command>
      <command> ls </command>
      <command> proplist </command>
      <command> propset </command>
      <command> revert </command>
      <command> stat </command>
      <command> update </command>
    </commands>
  </vcs>
  
  <vcs 
    name="git">

    <commands>
      <command> add </command>
      <command> blame </command>
      <command> commit </command>
      <command> diff </command>
      <command> help </command>
      <command> log </command>
      <command> push </command>
      <command> revert </command>
      <command> show </command>
      <command> status </command>
      <command> update </command>
    </commands>
  </vcs>

</vcss>
