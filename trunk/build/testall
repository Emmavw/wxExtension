# bash
# File:    testall
# Purpose: script to test all
# RCS-ID:  $Id$

target=$1
TESTDIR=./gccgtk2_dll

echo "-- test base --"
$TESTDIR/ex-test-base > test.log

echo "-- test app --"
$TESTDIR/ex-test-app >> test.log

echo "-- test report --"
$TESTDIR/ex-test-report >> test.log

svn commit -m "results of running testall"
